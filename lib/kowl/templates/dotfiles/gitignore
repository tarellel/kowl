# Ignore Vims files
*~
*.swp

# Ignore all logfiles and tempfiles.
.byebug_history
/log/*
/tmp/*
!/log/.keep
!/tmp/.keep

# Ignore uploaded files in development
/storage/*
!/storage/.keep

# Node / Yarn package files
node_modules
node_modules/
yarn-error.log

# Ignore master key for decrypting credentials and more.
/config/master.key

# Ignore application based files
.vagrant
